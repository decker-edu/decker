	isbn = {978-1-4503-4655-9},
	keywords = {exertion, exertion games, games, health, obesity},
	location = {Denver, Colorado, USA},
	numpages = {12},
	pages = {4899--4910},
	publisher = {ACM},
	series = {CHI '17},
	title = {Misrepresentation of Health Research in Exertion Games Literature},
