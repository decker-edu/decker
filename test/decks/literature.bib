	eprint = {http://pro.sagepub.com/content/48/23/2632.full.pdf+html},
	journal = {Proceedings of the Human Factors and Ergonomics Society Annual Meeting},
	keywords = {latency;cyber sickness;simulator sickness},
	number = {23},
	pages = {2632-2636},
	title = {Generalizeability of Latency Detection in a Variety of Virtual Environments},
