	doi = {http://doi.acm.org/10.1145/800248.807402},
	pages = {270--274},
	publisher = {ACM},
	title = {Casting curved shadows on curved surfaces},
	year = {1978},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://doi.acm.org/10.1145/800248.807402}}

