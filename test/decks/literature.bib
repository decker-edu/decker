@article{sommerville:2005,
	abstract = {
Adults and children readily construct action representations organized with respect to an ultimate goal. These representations allow one to predict the consequences of action, interpret and describe actions, and categorize action sequences. In this paper, we explore the ontogeny of hierarchically organized action representations, and its relation to infants' ability to produce similar sequences. To do so, we examine infants' perception and performance of a means-end sequence: pulling a cloth to retrieve a toy. Using a visual habituation paradigm, we demonstrate that 12-month-old infants understand that the initial step of the cloth-pulling sequence is directed toward the ultimate goal of attaining the toy, and use their knowledge of the causal constraints of the sequence to make this goal attribution. Ten-month-olds, however, appear transitional with respect to this understanding: their ability to identify the goal of the cloth-pulling sequence is related to their own ability to planfully solve a similar sequence. These findings are consistent with a burgeoning body of literature suggesting an intimate link between action production and perception, and suggest that this link is in place by at least 10 months of age.},
	author = {Sommerville, Jessica A. and Woodward, Amanda L.},
	date-added = {2015-10-13 15:04:34 +0000},
	date-modified = {2015-10-13 15:04:35 +0000},
	doi = {DOI: 10.1016/j.cognition.2003.12.004},
	issn = {0010-0277},
	journal = {Cognition},
	keywords = {Intentional structure},
	number = {1},
	pages = {1 - 30},
	title = {Pulling out the intentional structure of action: the relation between action processing and action production in infancy},
	url = {http://www.sciencedirect.com/science/article/B6T24-4C5PWHS-2/2/b9f95b4d021e5953ae71bc83f9792076},
	volume = {95},
	year = {2005},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/B6T24-4C5PWHS-2/2/b9f95b4d021e5953ae71bc83f9792076},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.cognition.2003.12.004}}

@inproceedings{soto_etal:a_semantic_approach:97,
	annote = {Michael Soto et al.: A Semantic Approach of Virtual Worlds Interoperability Abstract: In dem Zusammenwirken vers. virtuellen Umgebungen soll den auftretenden Problemen begegnet werden. Die Bedeutung von virtuellen Entit{\"a}ten in einer spez. VE wird duch ein Influence/Reaction Modell beschrieben, um die Beschreibung der Aktion klar zu trennen von den Konsequenzen der Aktion. In Erweiterung dazu soll zur Grundlegung gemeinsamen Wissens der Austausch und das Herleiten von Bedeutungen {\"u}ber den Abgleich durch Ontologien erm{\"o}glicht werden. 1. Introduction Verteilte VE k{\"o}nnen in allen Bereichen Verwendung finden => werden in Zukunft vielfach zum Einsatz kommen. Bisher sind dabei VE abgeschlossen in dem Sinne, dass User zweier verschiedener Umgebungen nicht zusammenarbeiten k{\"o}nnen. Als Anforderung f{\"u}r zuk{\"u}nftige VE entsteht aber gerade die M{\"o}glichkeit zur Zusammenarbeit zwischen verschiedenen VEs. Direkt dadurch wird die Frage der Heterogenit{\"a}t der Daten/ Umgebung aufgeworfen: Was bedeutet eine Aktion in einer VE in einer anderen VE unter anderen Gesichtspunkten (nicht als Klon der ersten, sondern gerade in VE, die sich auch durch ihr inneres Verhalten/ Erscheinungsbild unterscheiden) Wie l{\"a}sst sich ein Verhalten einer Entit{\"a}t auf eine solche andere VE {\"u}bertragen? > Die Semantik von abstrakteren Begriffen in der Ubertragung dann in einen Kontext hinein soll hier als L{\"o}sungsansatz vorgestellt werden. 2. Semantic in Virtual Worlds Semantik von VE ist auch von deren Zweck abh{\"a}ngig. Die Bedeutung einer Entit{\"a}t in einem speziellen VE ist auch abh{\"a}ngig von dessen Semantik (und Zweck). => Eine Entit{\"a}t kann verschiedenste Bedeutungen (Ubertragungen) haben in vers. VEs. > Die Semantik induziert das Verhalten eines Objektes. Auf zwei Punkte soll der Fokus hier gelegt werden: Beschreiben des Verhaltens: Influence/Reaction Modell Abgleich von Wissen (auch {\"u}ber Verhalten) {\"u}ber Ontologien > als high-level Konzept werden diese beiden Dinge unter dem Begriff Metaphor zusammengefasst. 3. The Influence/Reaction model Entit{\"a}ten sollen auserhalb ihres urspr{\"u}nglichen VE arbeiten k{\"o}nnen Daraus ergeben sich Konsequenzen: I. Das Ergebnis einer Aktion einer Entit{\"a}t ist nicht konstant sondern abh{\"a}ngig von der Umgebung (dem Kontext) und der Semantik der Entit{\"a}t. => Ergebnis der Aktion ist nicht Teil der Beschreibung der Aktion/ des Verhaltens Sondern: wird in der aktuellen VE direkt ausgef{\"u}hrt II. Die F{\"a}higkeiten/ Eigenschaften der Entit{\"a}t k{\"o}nnen in einer VE eingeschr{\"a}nkt sein. > Notwendig wird vor der Einbindung in eine Umgebung die Vorhersage der Kompatiblit{\"a}t => Angewendet wird das Influence/Reaction Modell: Definiert das Konzept der Aktion unter Trennung der Aktion von den Konsequenzen. 3.1. Die Regeln Zwei Arten von Regeln werden unterst{\"u}tzt: behavioral law - beschreiben das Verhalten einer Entit{\"a}t reaction law - geben die Ver{\"a}nderungen der Umgebung abh{\"a}ngig vom Kontext an (Grundregeln) => 2 Phasen in Aktionen: Einfl{\"u}sse auf Umgebung (Aktionen) und Reaktionen der Umgebung (s. Figure 1) Regeln bestehen aus Vorbedingungen ({\"u}ber dem Zustand der VE). Erweitert wird das Modell durch die Einf{\"u}hrung von resources. Diese repr{\"a}sentieren bedeutungstragende Eigenschaften der Umgebung (oder von Objekten oder Hintergundwissen). Resources k{\"o}nnen auch zur Konsistenzsicherung eingesetzt werden (Es gibt positive (= Existenz) und negative (=Verbot)). Der Ablauf (Fig. 1) im System ergibt sich: Aus Erf{\"u}llung von Vorbedingungen einer behavioral law werden Nachbedingungen produziert. > Deren Umsetzung in dem VE wird angeregt. => In Auswertung der Vorbedingungen der reaction laws werden nun auch die Nachbedingungen mit einbezogen und entsprechende Regeln angewendet > Nachbedingungen (versuchen VE zu modifizieren) 3.2. Example: Das hier eingef{\"u}hrte Beispiel wird in 3.3. und 5. fortgesetzt und verdeutlicht gut gerade den Einsatz von Ontologien zur Verbindung von VEs. 3.3. The Compatibility Evaluation Um das Zusammenarbeiten vers. VE erm{\"o}glichen zu k{\"o}nnen, m{\"u}ssen Entit{\"a}ten {\"u}berhaupt in beiden Umgebungen darstellbar sein = eine Semantik haben. Dieses wird durch die Kompatibili{\"a}t angegeben. Wird eine Entit{\"a}t in eine andere Umgebung {\"u}bertragen, wird ihr Grad der Kompatibilit{\"a}t {\"u}ber eine Funktion bestimmt abh{\"a}ngig von der Bedeutung der in den Regeln beteiligten Begriffe in der neuen virtuellen Umgebung. 4. Limitation of Influence/Reaction model Notwendig zur Ubertragung von Influence/Reaction Modellen auf verschiedene VEs ist ein Konsens {\"u}ber darin herrschende allgemeine Begriffe. > Ontologien sollen eingesetzt werden zum abstimmen der Begrifflichkeiten. 5. Enhancing virtual worlds interoperability with ontology Eine Ontologie soll eingesetzt werden als Vermittler zwischen den vers. VEs = dort wird der Begriffsvorrat definiert. Virtuelle Entit{\"a}ten werden mit semantic components assoziiert, die Angaben {\"u}ber Eigenschaften dieser angeben und Mapping-Regeln enthalten zwischen den Symbolen und den allgemeinen Konzepten in der Ontologie (s. Fig. 2: in den VE wird eine Entit{\"a}t auf der einen Seite konkret auch durch Werte beschrieben (ABox) - andererseits mit konzeptuellen Wissen des VE assoziiert (TBox). Dieses stellt ein Mapping auf allgemeine Defintion in der Ontologie bereit.). 6. Implementation 7. Conclusion and Further Works },
	author = {Soto, Michael},
	booktitle = {Proceedings of IEEE WET-ICE '97, Cambridge, MA},
