	date-added = {2015-10-13 15:04:34 +0000},
	date-modified = {2015-10-13 15:04:35 +0000},
	pages = {113-124},
	title = {{B}lickfixationstiefe in stereoskopischen {VR}-{U}mgebungen: {E}ine vergleichende {S}tudie},
	url = {http://trinity.inf.uni-bayreuth.de/download/Eyetracking-VRAR07.pdf},
	year = {2007},
